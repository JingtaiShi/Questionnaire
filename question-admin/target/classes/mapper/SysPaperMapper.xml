<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sjt.web.sys_paper.mapper.SysPaperMapper">
    <select id="listPaper" resultType="com.sjt.web.sys_paper.entity.SysPaper">
        select  * from sys_paper
        where
            question_id =#{questionId}
    </select>
</mapper>
